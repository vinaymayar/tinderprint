<html>
  <head>
  <% include ./partials/head %>
  </head>
  <body>
    <% include ./partials/header %>

    <div class="container">
      <div class="row" style="margin-top: 20px;">
        <div class="col-md-offset-3 col-md-6 col-xs-offset-1 col-xs-10">
          <div class="list-group">
            <% matches.forEach(function(candidate){ %>
              <% profile = candidate.profileImgPath; %>
              <% fingerprint = candidate.fingerprintImgPath; %>
              <%- include partials/match %>
            <% }); %>
            <% if(matches.length == 0 ) { %>
            <p> You don't have any matches yet.  Try looking at some more profiles, or give the
              people you liked
               some time to like you back!
            </p>
            <% } %>
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
